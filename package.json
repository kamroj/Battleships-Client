{
  "name": "battleships",
  "version": "1.0.3",
  "description": "Simple battle ship game, where players can play online on server",
  "main": "./game/main.js",
  "scripts": {
    "start": "electron .",
    "dist:linux": "electron-builder --linux"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Battle Ship",
    "Game",
    "Online"
  ],
  "author": "Spocone Sarny <sarny@spocone.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.1.3",
    "electron-builder": "^20.40.2",
    "electron-installer-debian": "^0.6.0",
    "electron-packager": "^9.1.0"
  },
  "dependencies": {
    "electron-css": "^0.13.3",
    "electron-reload": "^1.4.0",
    "jQuery": "^1.7.4",
    "xmlhttprequest": "^1.8.0"
  },
  "build": {
    "appId": "app",
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Utility"
    }
  }
